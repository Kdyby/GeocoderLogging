{
	"name": "kdyby/geocoder-logging",
	"type": "library",
	"description": "Provider decorator that catches willdurand/geocoder provider exceptions and pipes them to psr/log",
	"keywords": ["kdyby", "geocoder", "geocoding", "geo", "logging", "logger", "log"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/Kdyby/GeocoderLogging/issues"
	},
	"require": {
		"php": "^5.6 || ^7.0",
		"psr/log": "^1.0",
		"willdurand/geocoder": "^3.0"
	},
	"require-dev": {
		"nette/tester": "^1.7",
		"mockery/mockery": "^0.9"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Kdyby\\Geocoder\\Logging\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\Geocoder\\Logging\\": "tests/Logging/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
